@model IEnumerable<AssessoriaWeb.Models.PlanoAlimentar>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Atleta
        </th>
        <th>
            Nutricionista
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pln_datainicial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pln_datafinal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.pla_plano)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Atleta.Pessoa.pes_nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nutricionista.Pessoa.pes_nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pln_datainicial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pln_datafinal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.pla_plano)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.pln_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.pln_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.pln_id })
        </td>
    </tr>
}

</table>
